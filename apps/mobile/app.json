{
	"expo": {
		"name": "mobile",
		"slug": "mobile",
		"version": "1.0.0",
		"orientation": "portrait",
		"scheme": "mobile",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.pleasure1234.mobile"
		},
		"android": {
			"edgeToEdgeEnabled": true,
			"package": "com.pleasure1234.mobile"
		},
		"web": {
			"output": "static"
		},
		"plugins": ["expo-router"],
		"experiments": {
			"typedRoutes": true
		}
	}
}
